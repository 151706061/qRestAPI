#
# This module is provided as qMidasAPI_USE_FILE by qMidasAPIConfig.cmake.  It can
# be INCLUDED in a project to load the needed compiler and linker
# settings to use qMidasAPI.
#

IF(NOT qMidasAPI_USE_FILE_INCLUDED)
  SET(qMidasAPI_USE_FILE_INCLUDED 1)
  
  # Add include directories needed to use qMidasAPI.
  INCLUDE_DIRECTORIES(${qMidasAPI_INCLUDE_DIRS})
  
ENDIF()
